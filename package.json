{
  "name": "ohar.name",
  "version": "0.0.33",
  "description": "Site ohar.name",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env DEV=true webpack",
    "zip_old": "cd ./old_static_site && bestzip ./../dist/old_static_site.zip ./* && cd .. && echo 'Old site zipped'",
    "zip_spa": "cd ./dist/spa && bestzip ./../spa.zip ./* && cd ./../.. && echo 'SPA Zipped'",
    "deploy": "sh ./shell_scripts/deploy.sh && echo 'Deployed'",
    "build": "npm run build_spa && npm run build_old && echo 'Builded'",
    "build_old": "npm run zip_old && echo 'Old site builded'",
    "build_spa": "cross-env DEV=false webpack && npm run zip_spa echo 'SPA builded'",
    "update_site": "npm run build && npm run deploy && echo 'Site is updated'",
    "release": "npm version patch && npm run update_site && git push origin --tags && git push && echo 'Released'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ohar/Ohar.name.git"
  },
  "author": "Pavel Lysenko aka Ohar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ohar/Ohar.name/issues"
  },
  "homepage": "https://github.com/Ohar/Ohar.name#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bestzip": "^1.1.4",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "ejs-webpack-loader": "^2.2.2",
    "extract-loader": "^3.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^2.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.3.3",
    "webfontloader": "^1.6.26",
    "webpack": "3.12.0"
  },
